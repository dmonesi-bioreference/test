{
  "name": "@zolk/react-ui-starter-kit",
  "version": "1.2.0",
  "private": true,
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@storybook/react": "^5.2.8",
    "husky": "^3.1.0",
    "react": "^16.10.2",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-svg": "^11.0.5",
    "react-svg-spinner": "^1.0.4",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "build:storybook": "build-storybook",
    "build:components": "rm -rf dist && mkdir -p dist/components && babel -d dist src",
    "build:all": "yarn build:components",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "lint-styles": "stylelint 'src/**/*.ts'",
    "lint:fix": "yarn lint -- --fix",
    "plop": "plop",
    "start": "react-scripts start",
    "storybook": "start-storybook -s ./public -p 9009",
    "test": "react-scripts test --env=jsdom"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.7.7",
    "@storybook/addon-a11y": "^5.2.8",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-backgrounds": "^5.2.8",
    "@storybook/addon-docs": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-options": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/cli": "^5.2.8",
    "@storybook/preset-create-react-app": "^2.0.0",
    "@storybook/source-loader": "^5.2.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "^1.10.4",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-security": "^1.4.0",
    "lint-staged": "^9.5.0",
    "moment": "^2.24.0",
    "plop": "^2.4.0",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "storybook-react-router": "^1.0.8",
    "stylelint": "^12.0.1",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-prettier": "^1.1.2",
    "stylelint-processor-styled-components": "^1.9.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "stylelint",
      "eslint --fix",
      "prettier --write",
      "yarn test --bail --findRelatedTests --watchAll=false",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5",
    "semi": false
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier",
      "stylelint-config-styled-components"
    ],
    "plugins": [
      "stylelint-prettier"
    ],
    "rules": {
      "at-rule-no-unknown": null,
      "prettier/prettier": true
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "babel-plugin-styled-components"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
