{
  "name": "@zolk/react-storybook-starter-kit",
  "version": "0.2.5",
  "main": "dist/index.js",
  "dependencies": {
    "npm": "^5.8.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:storybook": "build-storybook",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009",
    "build:components": "rm -rf dist && mkdir -p dist/components && babel -d dist src",
    "build:sass": "node-sass src/sass/styles.scss -o dist/css/pre",
    "build:autoprefixer": "postcss dist/css/pre/styles.css --use autoprefixer -d dist/css",
    "build:all": "npm run build:components && npm run build:sass && npm run build:autoprefixer",
    "release": "npm run build:all && npm version patch && npm publish"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^3.4.0",
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-info": "^3.4.0",
    "@storybook/addon-knobs": "^3.4.0",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-cli": "^6.26.0",
    "babel-plugin-inline-react-svg": "^0.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-react": "^7.7.0",
    "node-sass": "^4.8.3",
    "postcss-cli": "^5.0.0",
    "postcss-loader": "^2.1.3",
    "sass-loader": "^6.0.7"
  }
}
