  "name": "project-pandas-prototype",
  "version": "0.0.0",
  "engines": {
    "node": ">=14"
  },
  "private": true,
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "auth:build-and-analyze": "node ./infrastructure/auth/scripts/build-and-analyze.js",
    "auth:deploy": "node ./infrastructure/auth/scripts/deploy.js",
    "build": "next build && next export",
    "build:next": "yarn make-icons && next build",
    "build:storybook": "build-storybook",
    "build:components": "rm -rf dist && mkdir -p dist/components && babel -d dist src",
    "build:all": "npm run build:components",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "lint-styles": "stylelint 'src/**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "lint:next": "next lint",
    "lint:typecheck": "tsc",
    "lint:all": "yarn lint && yarn lint-styles && yarn lint:next && yarn lint:typecheck",
    "make-icons": "node make-icons.js",
    "prebuild": "npm run make-icons",
    "plop": "plop",
    "start": "npm run make-icons && next dev",
    "start:next": "npm run build && next start",
    "storybook": "npm run make-icons && start-storybook -s ./public -p 9009",
    "cypress": "cypress run",
    "cypress:app": "cypress open",
    "test": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts test --env=jsdom",
    "test:ci": "npm run test:once && npm run test:e2e",
    "test:once": "cross-env SKIP_PREFLIGHT_CHECK=true CI=true react-scripts test --env=jsdom",
    "test:e2e": "start-server-and-test http://localhost:3000 cypress",
    "test:e2e:app": "start-server-and-test http://localhost:3000 cypress:app"
  },
