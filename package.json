{
  "name": "@zolk/react-ui-starter-kit",
  "version": "1.2.0",
  "private": true,
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "build:storybook": "build-storybook",
    "build:components": "rm -rf dist && mkdir -p dist/components && babel -d dist src",
    "build:all": "npm run build:components",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "lint-styles": "stylelint 'src/**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "make-icons": "node make-icons.js",
    "prebuild": "npm run make-icons",
    "plop": "plop",
    "start": "npm run make-icons && react-scripts start",
    "storybook": "npm run make-icons && start-storybook -s ./public -p 9009",
    "test": "react-scripts test --env=jsdom"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
